  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.9.6",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "@emajs/parser": "^1.0.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jsdom": "^16.2.3",
    "@types/jsonld": "^1.5.2",
    "@types/uuid": "^8.3.0",
    "csv-parse": "^4.15.3",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^16.3.0",
    "jsonld": "^4.0.1",
    "npm": "^6.14.7",
    "p-limit": "^3.1.0",
    "uuid": "^8.3.2"
  }
